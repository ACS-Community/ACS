    CommandLine=""
    ManagerReference=""
    ManagerRetry="10"
    ServerThreads="5"
    Timeout="30.0"
    UseIFR="1"
    xmlns="urn:schemas-cosylab-com:Container:1.0"
    xmlns:baci="urn:schemas-cosylab-com:BACI:1.0"
