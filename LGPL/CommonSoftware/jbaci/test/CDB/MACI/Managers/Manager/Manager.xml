		<cdb:_ string="ARCHIVE/TMCDB/MONITOR_CONTROL"/>
		<!-- cdb:_ string="AlarmService"/ --> 
	</Startup> 
	<ServiceComponents>
		<cdb:_ string="Log"/>
		<cdb:_ string="LogFactory"/>
		<cdb:_ string="NotifyEventChannelFactory"/>
		<cdb:_ string="InterfaceRepository"/>
		<cdb:_ string="CDB"/>
		<cdb:_ string="ACSLogSvc"/>
		<cdb:_ string="PDB"/>
	</ServiceComponents>

	<LoggingConfig immediateDispatchLevel="99">
		<log:_ Name="jacorb@Manager" minLogLevel="5" minLogLevelLocal="4"/>
	</LoggingConfig>

</Manager>


