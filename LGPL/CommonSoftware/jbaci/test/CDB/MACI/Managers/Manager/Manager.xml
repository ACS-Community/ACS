      Timeout="50.0"
      ClientPingInterval="10.0"
      ContainerPingInterval="10.0"
      AdministratorPingInterval="10.0">
	<Startup>
		<cdb:_ string="TEST_PS_1"/>
		<cdb:_ string="TEST_NONCOLLOCATED_PS_1"/>
		<cdb:_ string="FRIDGE1"/>
		<cdb:_ string="MOUNT1"/>
		<cdb:_ string="AMSSEQ1"/>
