<?xml version="1.0" encoding="UTF-8"?>
<BulkDataNTSender
   xmlns="urn:schemas-cosylab-com:BulkDataNTSender:1.0"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="urn:schemas-cosylab-com:BulkDataNTSender:1.0 BulkDataNTSender.xsd">

	<SenderStream Name="BLA">
		<Flow Name="BLA1">
			<DDSSenderFlowQoS>
				<datareader_qos name="BLA1">
				</datareader_qos>
			</DDSSenderFlowQoS>
		</Flow>
		<Flow Name="BLA2">
			<DDSSenderFlowQoS>
				<datareader_qos name="BLA2">
				</datareader_qos>
			</DDSSenderFlowQoS>
		</Flow>
		<DDSSenderStreamQoS>
			<participant_qos name="BLA">
			</participant_qos>
		</DDSSenderStreamQoS>
	</SenderStream>
	<SenderStream Name="PIPO">
		<Flow Name="PIPO1" SendFrameTimeout="0.3a"> <!-- Incorrect frame timeout value -->
			<DDSSenderFlowQoS>
				<datareader_qos name="PIPO1">
				</datareader_qos>
			</DDSSenderFlowQoS>
		</Flow>
		<Flow Name="PIPO3">
			<DDSSenderFlowQoS>
				<datareader_qos name="PIPO3">
				</datareader_qos>
			</DDSSenderFlowQoS>
		</Flow>
	</SenderStream>

</BulkDataNTSender>
