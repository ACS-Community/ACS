<Calendar xmlns="urn:schemas-cosylab-com:Calendar:1.0"
	  xmlns:baci="urn:schemas-cosylab-com:BACI:1.0"
          xmlns:cdb="urn:schemas-cosylab-com:CDB:1.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <yearAttributes description="yearAttribute"
               archive_min_int="5.0"
               archive_max_int="10.0" />
  <state description="state(0=late,1=up-to-date)"
               archive_min_int="5.0"
               archive_max_int="10.0" />
  <day description="day"
               archive_min_int="5.0"
               archive_max_int="10.0" />
  <month description="month"
               archive_min_int="5.0"
               archive_max_int="10.0" />
</Calendar>

