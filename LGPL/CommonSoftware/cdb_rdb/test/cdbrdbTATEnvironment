set ACS_TMP $env(PWD)/tmp
set env(ACS_TMP) $ACS_TMP
set env(ACS_LOG_STDOUT) 4 
set env(ACS_LOG_FILE) $ACS_TMP/log_cache.dat 
set env(ACS_STARTUP_TIMEOUT_MULTIPLIER) 10

set env(ENABLE_TMCDB) 1
set env(LOAD_FROM_XML) 1
set env(TMCDB_IN_MEMORY) 1
set env(TMCDB_CONFIGURATION_NAME) tatTest
set env(TMCDB_ACS_ONLY) 1
