<alarm-definition-list xmlns="urn:schemas-cosylab-com:Alarm:1.0"
                 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
                 xsi:schemaLocation="urn:schemas-cosylab-com:Alarm:1.0 ../../../../config/CDB/schemas/laser-definition.xsd">
	<alarm-definition fault-family="System" fault-member="ALARM_SOURCES" fault-code="1"/>
	
	<alarm-definition fault-family="AlarmSource" fault-member="ALARM_SOURCE_A" fault-code="1"/>
	<alarm-definition fault-family="AlarmSource" fault-member="ALARM_SOURCE_A" fault-code="2"/>
	<alarm-definition fault-family="ROdouble" fault-member="ALARM_SOURCE_A#current" fault-code="1"/>
	<alarm-definition fault-family="ROdouble" fault-member="ALARM_SOURCE_A#current" fault-code="2"/>
	
	<alarm-definition fault-family="AlarmSource" fault-member="ALARM_SOURCE_B" fault-code="1"/>
	<alarm-definition fault-family="AlarmSource" fault-member="ALARM_SOURCE_B" fault-code="2"/>
	<alarm-definition fault-family="ROdouble" fault-member="ALARM_SOURCE_B#current" fault-code="1"/>
	<alarm-definition fault-family="ROdouble" fault-member="ALARM_SOURCE_B#current" fault-code="2"/>
    <alarm-definition fault-family="AlarmSource" fault-member="ALARM_SOURCE_PS" fault-code="1" />
	<alarm-definition fault-family="AlarmSource" fault-member="ALARM_SOURCE_MOUNT" fault-code="1" />
	<alarm-definition fault-family="AlarmSource" fault-member="ALARM_SOURCE_ANTENNA" fault-code="1" />
	<alarm-definition fault-family="AlarmSource" fault-member="ALARM_SOURCE_MOUNTCPP" fault-code="1" />
</alarm-definition-list>
