clean : clean_all 
	$(RM) *~ CERNAlarmSystemInterfaceProxy/*~ CERNAlarmSystemInterfaceProxy/*.pyc CERNAlarmSystemInterfaceProxy/*.pyo
	@echo " . . . clean done"

clean_dist : clean_all clean_dist_all 
	@echo " . . . clean_dist done"

man   : do_man 
	@echo " . . . man page(s) done"

install : install_all
	@echo " . . . installation done"


