<CRD xmlns="urn:schemas-cosylab-com:CRD:1.0"
     xmlns:baci="urn:schemas-cosylab-com:BACI:1.0"
     xmlns:cdb="urn:schemas-cosylab-com:CDB:1.0"
     xmlns:amb="urn:schemas-cosylab-com:AmbDevice:1.0"
     xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <maserVsGPSCounter/>
  <maserCounter/>
  <setResetTime/>
  <getResetTime/>
  <efc5MHz>
    <amb:rcaMap relative_address="6" data_length="2">
      <amb:doubleBitMap numberOfBits="12" fieldWidth="16" scale="0.0048840049" signedData="true"/>
    </amb:rcaMap>
  </efc5MHz>

  <pwr5MHz>
    <amb:rcaMap relative_address="7" data_length="2">
      <amb:doubleBitMap numberOfBits="12" fieldWidth="16" scale="0.0048840049" signedData="true"/>
    </amb:rcaMap>
  </pwr5MHz>

  <pwr25MHz>
    <amb:rcaMap relative_address="8" data_length="2">
      <amb:doubleBitMap numberOfBits="12" fieldWidth="16" scale="0.0048840049" signedData="true"/>
    </amb:rcaMap>
  </pwr25MHz>

  <pwr125MHz>
    <amb:rcaMap relative_address="9" data_length="2">
      <amb:doubleBitMap numberOfBits="12" fieldWidth="16" scale="0.0048840049" signedData="true"/>
    </amb:rcaMap>
  </pwr125MHz>

  <pwr2GHz>
    <amb:rcaMap relative_address="10" data_length="2">
      <amb:doubleBitMap numberOfBits="12" fieldWidth="16" scale="0.0048840049" signedData="true"/>
    </amb:rcaMap>
  </pwr2GHz>

  <laserCurrent>
    <amb:rcaMap relative_address="12" data_length="2">
      <amb:doubleBitMap numberOfBits="12" fieldWidth="16" scale="0.4884004884" signedData="true"/>
    </amb:rcaMap>
  </laserCurrent>

  <vdcMinus5>
    <amb:rcaMap relative_address="13" data_length="2">
      <amb:doubleBitMap numberOfBits="12" fieldWidth="16" scale="0.0048840049" signedData="true"/>
    </amb:rcaMap>
  </vdcMinus5>

  <vdc15>
    <amb:rcaMap relative_address="14" data_length="2">
      <amb:doubleBitMap numberOfBits="12" fieldWidth="16" scale="0.0097680098" signedData="true"/>
    </amb:rcaMap>
  </vdc15>

  <status/>
  <xilinxPromReset/>
  <Address/>
  <errors/>
  <transactions/>
  <temperature/>
  <last_error_code description="Last error code recorded in AMBSI CAN hardware"
                  format      ="%8x"
                  units       ="none"
                  resolution  ="65535">
    <amb:rcaMap relative_address = "196609" data_length="4">
      <amb:intBitMap numberOfBits="32" fieldWidth="32"/>
    </amb:rcaMap>
  </last_error_code>
  <protocol_rev_level/>
</CRD>

