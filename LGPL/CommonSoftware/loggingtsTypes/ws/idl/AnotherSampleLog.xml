<?xml version="1.0" encoding="ISO-8859-1"?>
<LogDefinitionType 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns="Alma/ACSLogTS"
	name="AnotherSampleLog" 
	type="10"
	shortDescription="Example LogDefinitionType"
	description="Longer description of the sample LogDefinitionType"
	URL="http://someurl.org"
	_prefix="alma"
        xsi:schemaLocation="Alma/ACSLogTS ACSLogTS.xsd">
	
	<LogDefinition
		logName="logWithMembers"
		shortDescription="Log with members"
		description="This is a somewhat more complex log using members"
		URL="http://someurl.org"
		priority="WARNING"
		audience="Zaphod Beeblebrox">
		<Member name="someStringMember"
			type="string"
			description="member description"/>
		<Member name="someLongMember"
			type="long"
			description="member description"/>
		<Member name="someDoubleMember"
			type="double"
			description="member description"/>
	</LogDefinition>
</LogDefinitionType>

