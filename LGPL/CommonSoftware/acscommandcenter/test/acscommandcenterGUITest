#! /bin/bash
CLASSPATH=`vltMakeJavaClasspath`
IP=`host $HOST | awk '{print $4}'`

if [ x$1 = xmanual ] ; then
   mkdir -p tmp
   common_options="-variable IP=$IP -variable HOST=$HOST"
   manual_options="-variable HOME=. -systemcfg ./qftestJUI.cfg"
   qftestJUI $common_options $manual_options acscommandcenter.qft
   
else
   qftestJUI -batch  -report.copyicons -report.depth 10 -report.html %b.html -systemcfg $HOME/qftestJUI.cfg -variable IP=$IP -variable HOST=$HOST -variable HOME=$HOME -libpath $HOME -runlog %b.qrz acscommandcenter.qft
   
fi

echo $?
