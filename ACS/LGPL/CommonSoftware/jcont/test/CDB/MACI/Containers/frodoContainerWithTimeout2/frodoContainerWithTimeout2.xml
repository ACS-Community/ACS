<?xml version="1.0" encoding="ISO-8859-1"?>

<!-- Java container which overrides the default client-side ORB timeout: 
200 seconds, more than system-default timeout -->
<Container 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns="urn:schemas-cosylab-com:Container:1.0" 
	xmlns:log="urn:schemas-cosylab-com:LoggingConfig:1.0" 
	ImplLang="java"
	Timeout="10"
	>

	<Autoload>
	</Autoload>

	<LoggingConfig
		minLogLevel="2"
		minLogLevelLocal="2" >
		<log:_ Name="jacorb@frodoContainerWithTimeout2" minLogLevel="5" minLogLevelLocal="5" />
	</LoggingConfig>

</Container>
