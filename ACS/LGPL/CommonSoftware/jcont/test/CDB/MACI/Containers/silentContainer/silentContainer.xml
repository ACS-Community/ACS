<?xml version="1.0" encoding="ISO-8859-1"?>

<Container 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns="urn:schemas-cosylab-com:Container:1.0" 
	xmlns:log="urn:schemas-cosylab-com:LoggingConfig:1.0" 
	ImplLang="java"
	>
    
    <Autoload>
    </Autoload>
    
    <LoggingConfig
    	minLogLevel="3"
    	minLogLevelLocal="8"
    	centralizedLogger="Log"
    	maxLogQueueSize="1000"
    	immediateDispatchLevel="4"
    	dispatchPacketSize="5"
    	flushPeriodSeconds="10"
    	>
    	<log:_ Name="jacorb@silentContainer" minLogLevel="3" minLogLevelLocal="8" />
        <log:_ Name="DefaultDataReceiver" minLogLevel="5" minLogLevelLocal="5" />
    </LoggingConfig>

</Container>
