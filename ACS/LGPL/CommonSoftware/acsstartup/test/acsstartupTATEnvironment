set HOST [eval exec "hostname -s"]

set env(ACS_STARTUP_TIMEOUT_MULTIPLIER) 2
set env(ACS_LOG_STDOUT) 4
set env(ACS_TMP) $env(PWD)/tmp/$HOST
set env(PYTHONPATH) $env(PWD)/../src:$env(PYTHONPATH)
