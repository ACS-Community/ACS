	<LTA_CAN_ErrorCount description="LTA CAN bus errors count" units=""/>
	<SCC_CAN_ErrorCount description="SCC CAN bus errors count" units=""/>
	<QCC_CAN_ErrorCount description="QCC CAN bus errors count" units=""/>

