<Door xmlns="urn:schemas-cosylab-com:Door:1.0"
      xmlns:baci="urn:schemas-cosylab-com:BACI:1.0"
      xmlns:cdb="urn:schemas-cosylab-com:CDB:1.0"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <ref_position
               min_value="0.0"
               max_value="100.0"
               default_timer_trig="1.0"
               min_timer_trig="0.001"
               min_delta_trig="0.01526"
 	       default_value="0.0"
	       graph_min="0.0"
	       graph_max="1000.0"
	       min_step="0.01526"
               description="Ref_position"
    	       format="%9.4f"
    	       resolution="65535"/>
  <position
	    alarm_low_on="10.0"
	    alarm_low_off="20.0"
	    alarm_high_on="990.0"
	    alarm_high_off="980.0"
	    default_timer_trig="1.0"
	    min_timer_trig="0.001"
	    min_delta_trig="0.01526"
	    default_value="0.0"
	    graph_min="0.0"
	    graph_max="1000.0"
	    min_step="0.01526"
    	    description="Position"
    	    format="%9.4f"
    	    resolution="65535"/>
  <substate
	    default_value="0"
            description="Substate"           
	    format="%d"
            resolution="3263"/>
  <version  description="VersionNumber"/>

</Door>

